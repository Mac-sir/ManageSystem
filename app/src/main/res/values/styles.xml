<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--&lt;!&ndash; Customize your theme here. &ndash;&gt;-->
        <!--<item name="colorPrimary">@color/colorPrimary</item>-->
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
        <!--<item name="android:textAllCaps">false</item>-->
        <!--<item name="android:windowActionBar">false</item>-->
        <!--<item name="windowActionBar">false</item>-->
        <!--<item name="windowNoTitle">true</item>-->
        <!--<item name="android:windowIsTranslucent">true</item>-->

        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!--<item name="android:windowIsTranslucent">true</item>-->
    </style>

    <style name="TabStyle" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="topbar_bg">
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/topbar_height</item>
    </style>

    <style name="topbar_bg_green" parent="topbar_bg">
        <item name="android:background">@color/green</item>
    </style>

    <!-- 用于开启滑动返回功能的 Activity -->
    <style name="AppTheme.Transparent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.NoBar">
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="news_content">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="AnimationActivity" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/slide_in_right</item>
        <item name="android:activityOpenExitAnimation">@anim/anim_no</item>
        <item name="android:activityCloseEnterAnimation">@anim/anim_no</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_out_right</item>
        <!--<item name="android:activityOpenEnterAnimation">@anim/slide_in_left</item>-->
        <!--<item name="android:activityOpenExitAnimation">@anim/slide_out_left</item>-->
    </style>

    <!--splashTheme-->
    <style name="splashTheme" parent="AppTheme">
        <item name="android:windowBackground">@color/title_bar</item>
    </style>

    <style name="TextStyle111">
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">3dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:visibility">invisible</item>
    </style>

    <style name="AppBaseTheme" parent="android:Theme.Holo.Light.NoActionBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- 以后新增的任何系统组件(View)名都用简写（取单词首字母，SurfaceView简写为sfv）；其它都不简写 -->
    <style name="content_item">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">@dimen/content_item_left_tv_padding_left</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/to_alpha</item>
    </style>

    <style name="content_item_white" parent="content_item">
        <item name="android:background">@drawable/white_to_gray</item>
    </style>

    <style name="text_middle" parent="text">
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <style name="content_item_lf_tv" parent="text_middle">
        <item name="android:paddingRight">@dimen/padding_big</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="content_item_rt_img" parent="text_middle">
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">@dimen/padding_micro</item>
        <item name="android:src">@drawable/arrow_right</item>
    </style>

    <style name="wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="match_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="wrap_match">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="match_wrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- page <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="activity_page">
        <item name="android:splitMotionEvents">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/activity_bg</item>
        <item name="android:orientation">vertical</item>
        <!-- 状态栏沉浸 <<<<<<<<<<<<<<<<< -->
        <item name="android:clipToPadding">true</item>
        <item name="android:fitsSystemWindows">true</item>
        <!-- 状态栏沉浸 >>>>>>>>>>>>>>>>> -->
    </style>

    <style name="bottom_window_page" parent="activity_page">
        <item name="android:gravity">bottom|center_horizontal</item>
        <item name="android:background">@color/alpha_complete</item>
    </style>

    <!-- page >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


    <!-- blank <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="blank">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
    </style>

    <style name="blank_small" parent="blank">
        <item name="android:layout_height">5dp</item>
    </style>

    <style name="blank_middle" parent="blank">
        <item name="android:layout_height">10dp</item>
    </style>

    <style name="blank_big" parent="blank">
        <item name="android:layout_height">15dp</item>
    </style>

    <style name="blank_huge" parent="blank">
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="blank_large" parent="blank">
        <item name="android:layout_height">30dp</item>
    </style>

    <style name="blank_item" parent="blank">
        <item name="android:layout_height">15dp</item>
    </style>

    <style name="blank_bottom" parent="blank">
        <item name="android:layout_height">30dp</item>
    </style>

    <!-- blank >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


    <!-- text <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="text" parent="wrap_wrap">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/gray_3</item>
    </style>

    <style name="text_title" parent="text">
        <item name="android:textSize">22sp</item>
    </style>

    <!-- text >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


    <!-- Button <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="img_btn" parent="wrap_wrap">
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="img_center" parent="wrap_wrap">
        <item name="android:scaleType">center</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="img_centercrop" parent="wrap_wrap">
        <item name="android:scaleType">centerCrop</item>
